{
  "name": "deep-time",
  "version": "1.0.0",
  "description": "Mobile AR application for geological and historical layer visualization",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:properties": "vitest run --testPathPattern=properties",
    "test:unit": "vitest run --testPathPattern=unit"
  },
  "keywords": [
    "ar",
    "geology",
    "visualization"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "fast-check": "^3.14.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
